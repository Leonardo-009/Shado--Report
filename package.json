{
    "name": "shadow-report",
    "version": "1.0.0",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-win": "electron-packager . shadow-report --platform=win32 --arch=x64 --out=dist --overwrite --icon=./shadow.ico"
    },
    "devDependencies": {
        "electron": "^30.0.0",
        "electron-packager": "^17.1.1"
    },
  "build": {
    "appId": "com.example.my-electron-app",
    "win": {
      "target": "nsis",
      "icon": "shadow.ico"
    },
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}